@{
    string wrapID = Guid.NewGuid().ToString("N");
}

@model Base.UI.ViewModal.ViewModelConfig

<div class="container">
    <form id="@wrapID">
        <div class="row center-align-text">
            <div class="col-lg-12">
                <h3>ViewModelConfig</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <dl class="dl-horizontal">
                    <dt>Мнемоника</dt>
                    <dd>@Model.Mnemonic</dd>

                    <dt>Наименование</dt>
                    <dd>@Model.Title</dd>

                    <dt>Сервис</dt>
                    <dd>@Model.ServiceType.Name</dd>

                </dl>
            </div>
        </div>
    </form>
</div>
